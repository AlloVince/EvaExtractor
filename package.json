{
  "name": "evaextractor",
  "version": "0.0.0-development",
  "description": "A toolset for store/extract/transfer/iterator data in local disk/Ali cloud oss/AWS S3/minio",
  "main": "index.js",
  "scripts": {
    "ava": "LOG_LEVEL=error NODE_ENV=test ava --verbose --serial",
    "test": "nyc -a --reporter=lcov --reporter=text --reporter=html npm run ava",
    "lint": "tslint src/**/*.ts --exclude '**/*.d.ts' --project tsconfig.json",
    "watch": "tsc -w",
    "build": "rm -rf lib && tsc -m \"System\" --outFile \"lib/index\" --sourceMap \"false\" && rm lib/index && mv lib/index.d.ts ./ && tsc",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "license": "MIT",
  "author": "",
  "homepage": "",
  "engines": {
    "node": ">=10.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "sources": [
      "src/**/*.ts"
    ],
    "failFast": false,
    "tap": false,
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "babel": false
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib/**/*"
    ],
    "sourceMap": true
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "get-stream": "^4.0.0",
    "mkdirp": "^0.5.1"
  },
  "peerDependencies": {
    "ali-oss": "^5.3.1",
    "fast-glob": "^2.2.2",
    "minio": "^7.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/get-stream": "^3.0.1",
    "@types/lodash": "^4.14.116",
    "@types/minio": "^7.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.11.3",
    "@types/sequelize": "^4.27.26",
    "ava": "^1.0.0-beta.8",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2",
    "semantic-release": "^15.9.16",
    "travis-deploy-once": "^5.0.9",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
